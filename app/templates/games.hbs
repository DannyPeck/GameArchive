<div class="GameContainer">
  <h1>Game List</h1>

  <h1>Login</h1>
  <span>title: </span> {{input type="text" value=title}}<br><br>
  <span>system: </span> {{input type="text" value=system}}<br><br>
  <span>year: </span> {{input type="text" value=year}}
  <br>

  <button id="add" {{action "add" title system year}}>
    add
  </button>

  {{#each model as |entry|}}
    <p><strong>-----------------------</strong></p>
    {{game-entry game=entry}}
  {{/each}}
</div>
